{
  "name": "tools",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run ci && vite build",
    "build:only": "vue-tsc --noEmit && vite build",
    "ci": "npm run lint:check && npm run test:run && vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "oxlint --fix && prettier --write .",
    "lint:check": "oxlint && prettier --check ."
  },
  "dependencies": {
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/ui": "^2.1.9",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^24.1.1",
    "oxlint": "^1.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.10"
  },
  "volta": {
    "node": "22.14.0"
  }
}
